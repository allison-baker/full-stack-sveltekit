    "vitest": "^1.2.2"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.26.3",
    "@auth/mongodb-adapter": "^2.3.3",
    "@auth/sveltekit": "^0.12.3",
    "@floating-ui/dom": "1.6.1",
    "chart.js": "^4.4.2",
    "dotenv": "^16.4.1",
    "highlight.js": "11.9.0",
    "marked": "^12.0.2",
